# The code Works

score = input("Enter Score: ")

score = float(score)

if score > 1.0:
    print('ERROR')

elif float(1.0) >= score >= float(0.9):
    print('A')
elif float(0.9) >= score >= float(0.8):
    print('B')
elif float(0.8) >= score >= float(0.7):
    print('C')
elif float(0.7) >= score >= float(0.6):
    print('D')
elif float(0.6) > score:
    print('F')
